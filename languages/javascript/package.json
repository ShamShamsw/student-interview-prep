{
  "name": "interview-prep-javascript",
  "version": "1.0.0",
  "description": "JavaScript solutions for interview prep problems",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint solutions/ tests/",
    "lint:fix": "eslint solutions/ tests/ --fix"
  },
  "keywords": [
    "interview",
    "algorithms",
    "data-structures",
    "javascript",
    "leetcode"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "solutions/**/*.js",
      "!solutions/**/*.test.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
